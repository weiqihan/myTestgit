<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- 1 配置类和表对应 
		class标签
		name属性：实体类全路径
		table属性：数据库表名称
	-->
	<class name="com.zzu.entity.Society" table="t_society">
		<id name="societyid" column="societyid">
			<generator class="native"></generator>
		</id>
		<property name="imgUrl" column="imgUrl"></property>
		<property name="title" column="title"></property>
		<property name="time" column="time"></property>
		<property name="readerNumber" column="readerNumber"></property>
		<property name="News_Id" column="News_Id"></property>
	</class>
</hibernate-mapping>